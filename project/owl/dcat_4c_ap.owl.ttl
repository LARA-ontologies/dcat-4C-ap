        skos:title ;
    skos:inScheme <https://w3id.org/StroemPhi/dcat-4C-ap> .

nfdi:type a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "type" ;
    skos:definition "The slot to specify the rdf:type of an entity according to a controlled vocabulary." ;
    skos:inScheme <https://w3id.org/StroemPhi/dcat-4C-ap> .

